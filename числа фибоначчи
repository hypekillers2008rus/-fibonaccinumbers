#include <stdio.h>

int main() {
    int n, i;
    unsigned long long int first = 0, second = 1, next;

    printf("n= ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");

    for (i = 0; i < n; i++) {
        if (i <= 1) {
            next = i;
        } else {
            next = first + second;
            first = second;
            second = next;
        }
        printf("%llu ", next);
    }

    return 0;
}
